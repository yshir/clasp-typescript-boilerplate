{
  "name": "clasp-typescript-boilerplate",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "clasp:login": "./node_modules/.bin/clasp login",
    "clasp:push": "./node_modules/.bin/clasp push",
    "clasp:pull": "./node_modules/.bin/clasp pull",
    "dev": "./node_modules/.bin/ts-node src/index.ts",
    "eslint:fix": "eslint . --ext .js,.ts,.jsx,.tsx --fix",
    "prettier:fix": "prettier --write .",
    "prepare": "husky install",
    "lint-staged": "lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": [
      "prettier --write .",
      "eslint --fix"
    ]
  },
  "license": "MIT",
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^2.0.1",
    "@types/google-apps-script": "^1.0.30",
    "@types/node": "^14.14.36",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "husky": "^5.2.0",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "@google/clasp": "^2.3.0"
  }
}
